<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#212121"
    android:backgroundTint="#212121">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#212121"
        android:backgroundTint="#212121">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="30dp"
            android:background="#242424"
            android:backgroundTint="#242424"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#242424"
                android:backgroundTint="#242424"
                android:orientation="horizontal"
                android:padding="5dp">

                <TextView
                    android:layout_width="250dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#242424"
                    android:backgroundTint="#242424"
                    android:fontFamily="monospace"
                    android:inputType="textMultiLine"
                    android:paddingStart="20dp"
                    android:paddingTop="2dp"
                    android:paddingBottom="2dp"
                    android:text="Date Submitted"
                    android:textAlignment="textEnd"
                    android:textColor="#FFEBEE"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/txtDateSubmitted"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#242424"
                    android:backgroundTint="#242424"
                    android:fontFamily="monospace"
                    android:gravity="center"
                    android:inputType="textMultiLine"
                    android:paddingStart="10dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#FFEBEE"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#242424"
                android:backgroundTint="#242424"
                android:orientation="horizontal"
                android:padding="5dp">

                <TextView
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#242424"
                    android:backgroundTint="#242424"
                    android:fontFamily="monospace"
                    android:inputType="textMultiLine"
                    android:paddingStart="20dp"
                    android:paddingTop="2dp"
                    android:paddingBottom="2dp"
                    android:text="Full Name"
                    android:textAlignment="textEnd"
                    android:textColor="#FFFFFF"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/txtFullName"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#242424"
                    android:backgroundTint="#242424"
                    android:fontFamily="monospace"
                    android:gravity="center"
                    android:inputType="textMultiLine"
                    android:paddingStart="10dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#FFEBEE"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#242424"
                android:backgroundTint="#242424"
                android:orientation="horizontal"
                android:padding="5dp">

                <TextView
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#242424"
                    android:backgroundTint="#242424"
                    android:fontFamily="monospace"
                    android:inputType="textMultiLine"
                    android:paddingStart="20dp"
                    android:paddingTop="2dp"
                    android:paddingBottom="2dp"
                    android:text="Gender"
                    android:textAlignment="textEnd"
                    android:textColor="#FFEBEE"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/txtGender"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#242424"
                    android:backgroundTint="#242424"
                    android:fontFamily="monospace"
                    android:gravity="center"
                    android:inputType="textMultiLine"
                    android:paddingStart="10dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#FFEBEE"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#242424"
                android:backgroundTint="#242424"
                android:orientation="horizontal"
                android:padding="5dp">

                <TextView
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#242424"
                    android:backgroundTint="#242424"
                    android:fontFamily="monospace"
                    android:inputType="textMultiLine"
                    android:paddingStart="20dp"
                    android:paddingTop="2dp"
                    android:paddingBottom="2dp"
                    android:text="Selected Appointment Date"
                    android:textAlignment="textEnd"
                    android:textColor="#FFFFFF"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/txtAppointmentDate"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#242424"
                    android:backgroundTint="#242424"
                    android:fontFamily="monospace"
                    android:gravity="center"
                    android:inputType="textMultiLine"
                    android:paddingStart="10dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#FFEBEE"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#242424"
                android:backgroundTint="#242424"
                android:orientation="horizontal"
                android:padding="5dp">

                <TextView
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#242424"
                    android:backgroundTint="#242424"
                    android:fontFamily="monospace"
                    android:inputType="textMultiLine"
                    android:paddingStart="20dp"
                    android:paddingTop="2dp"
                    android:paddingBottom="2dp"
                    android:text="Selected Appointment Time"
                    android:textAlignment="textEnd"
                    android:textColor="#FFEBEE"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/txtAppointmentTime"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#242424"
                    android:backgroundTint="#242424"
                    android:fontFamily="monospace"
                    android:gravity="center"
                    android:inputType="textMultiLine"
                    android:paddingStart="10dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#FFEBEE"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#242424"
                android:backgroundTint="#242424"
                android:orientation="horizontal"
                android:padding="5dp">

                <TextView
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#242424"
                    android:backgroundTint="#242424"
                    android:fontFamily="monospace"
                    android:inputType="textMultiLine"
                    android:paddingStart="20dp"
                    android:paddingTop="2dp"
                    android:paddingBottom="2dp"
                    android:text="Selected Service"
                    android:textAlignment="textEnd"
                    android:textColor="#FFFFFF"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/txtSelectedService"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#242424"
                    android:backgroundTint="#242424"
                    android:fontFamily="monospace"
                    android:gravity="center"
                    android:inputType="textMultiLine"
                    android:paddingStart="10dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#FFEBEE"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#242424"
                android:backgroundTint="#242424"
                android:orientation="horizontal"
                android:padding="5dp">

                <TextView
                    android:layout_width="250dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#242424"
                    android:backgroundTint="#242424"
                    android:fontFamily="monospace"
                    android:inputType="textMultiLine"
                    android:paddingStart="20dp"
                    android:paddingTop="2dp"
                    android:paddingBottom="2dp"
                    android:text="Time Submitted"
                    android:textAlignment="textEnd"
                    android:textColor="#FFEBEE"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/txtTimeSubmitted"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#242424"
                    android:backgroundTint="#242424"
                    android:fontFamily="monospace"
                    android:gravity="center"
                    android:inputType="textMultiLine"
                    android:paddingStart="10dp"
                    android:text="TextView"
                    android:textAlignment="center"
                    android:textColor="#FFEBEE"
                    android:textSize="14sp" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</RelativeLayout>